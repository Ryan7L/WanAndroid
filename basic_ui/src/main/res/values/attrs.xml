<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="StatusBarView" tools:ignore="ResourceName">
        <attr name="sbv_transparent">
            <enum name="unchanged" value="0" />
            <enum name="transparent" value="1" />
            <enum name="unTransparent" value="2" />
        </attr>
        <attr name="sbv_iconMode">
            <enum name="unchanged" value="0" />
            <enum name="darkIcon" value="1" />
            <enum name="lightIcon" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="PercentImageView" tools:ignore="ResourceName">
        <attr name="piv_basics" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
        <attr name="piv_percent" format="float" />
    </declare-styleable>
</resources>